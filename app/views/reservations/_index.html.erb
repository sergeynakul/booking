<h2>Reservations</h2>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th>Duration</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @table.reservations.each do |reservation| next if reservation.id.nil? %>
      <tr>
        <td><%= reservation.start %></td>
        <td><%= reservation.duration %></td>
        <td><%= reservation.user %></td>
        <td><%= link_to 'Show', reservation_path(reservation) %></td>
        <td><%= link_to 'Edit', edit_reservation_path(reservation) %></td>
        <td><%= link_to 'Destroy', reservation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
